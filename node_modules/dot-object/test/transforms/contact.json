{
  "name": "Contact Transform",
  "input": {
    "id": 1,
    "contact": {
      "firstName": "John",
      "lastName": "Doe",
      "email": "example@test.com"
    }
  },
  "transform": {
    "id": "nr",
    "contact.firstName": "name.first",
    "contact.lastName": "name.last",
    "contact.email": "email"
  },
  "expected": {
    "nr": 1,
    "name": {
      "first": "John",
      "last": "Doe"
    },
    "email": "example@test.com"
  }
}

